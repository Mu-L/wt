<?xml version="1.0" encoding="UTF-8"?>
<messages xmlns:if="Wt.WTemplate.conditions">
  <message id="Wt.Auth.field.active">
    <![CDATA[
    <div class="row mb-3">
      <label for="${id:{1}}" class="col-sm-2 col-form-label text-sm-end" style="min-width: 100px;">
        ${tr:Wt.Auth.{1}}
      </label>
      <div class="col-sm-8">
        ${{1}}
        <div class="form-text">
          ${{1}-info}
        </div>
      </div>
    </div>
    ]]>
  </message>

  <message id="Wt.Auth.field-check">
    <![CDATA[
    ${<if:{1}>}
    <div class="row mb-3">
      <label for="${id:{1}}" class="col-sm-2 col-form-label text-sm-end" style="min-width: 100px;">
        ${tr:Wt.Auth.{1}}
      </label>
      <div class="col-sm-8">
        ${{1}}
        <span class="form-text">
          ${{1}-info}
        </span>
      </div>
    </div>
    ${</if:{1}>}
    ]]>
  </message>

  <message id="Wt.Auth.field2.active">
    <![CDATA[
    <div class="row mb-3">
      <label for="${id:{1}}" class="col-sm-2 col-form-label text-sm-end" style="min-width: 100px;">
        ${{2}}
      </label>
      <div class="col-sm-8">
        ${{1}}
        <span class="form-text">
          ${{1}-info}
        </span>
      </div>
    </div>
    ]]>
  </message>

  <message id="Wt.Auth.field3.active">
    <![CDATA[
    <div class="row mb-3">
      <label for="${id:{1}}" class="col-sm-2 col-form-label text-sm-end" style="min-width: 100px;">
        ${{2}}
      </label>
      <div class="col-sm-8">
        ${{1}}
        <span class="form-text">
          ${{1}-info} ${{3} class="btn-link"}
        </span>
      </div>
    </div>
    ]]>
  </message>

  <message id="Wt.Auth.checkbox.field">
    <![CDATA[
    ${<if:{1}>}
    <div class="row mb-3">
      <div class="offset-sm-2 col-sm-8">
        <div class="form-check">
          ${{1}}
          <label for="${id:{1}}" class="form-check-label">
            ${tr:Wt.Auth.{1}}
          </label>
          <div class="form-text">
              ${{1}-info}
          </div>
        </div>
      </div>
    </div>
    ${</if:{1}>}
    ]]>
  </message>

  <message id="Wt.WNavigationBar.template">
    <div class="container-fluid">
      ${title-link}
      ${collapse-button}
      ${contents}
    </div>
  </message>

  <message id="Wt.WNavigationBar.expand-button">
    <span class="navbar-toggler-icon"></span>
  </message>

  <message id="Wt.WDialog.template">
    <div class="modal-dialog">
      ${layout}
    </div>
  </message>

  <message id="Wt.WDialog.titlebar">
    <h4 class="modal-title">${title}</h4>
  </message>
</messages>
