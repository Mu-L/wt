<?xml version="1.0" encoding="UTF-8"?>
<messages xmlns:if="Wt.WTemplate.conditions">
  <message id="Wt.Auth.field.active">
    <![CDATA[
    <div class="row mb-3">
      <label for="${id:{1}}" class="col-form-label">
        ${tr:Wt.Auth.{1}}
      </label>
      <div class="">
        ${{1}}
      </div>
      <div class="form-text">
        ${{1}-info}
      </div>
    </div>
    ]]>
  </message>

  <message id="Wt.Auth.field-check">
    <![CDATA[
    ${<if:{1}>}
    <div class="row mb-3">
      <label for="${id:{1}}" class="col-form-label">
        ${tr:Wt.Auth.{1}}
      </label>
      <div class="">
        ${{1}}
      </div>
      <div class="form-text">
        ${{1}-info}
      </div>
    </div>
    ${</if:{1}>}
    ]]>
  </message>

  <message id="Wt.Auth.field2.active">
    <![CDATA[
    <div class="row mb-3">
      <label for="${id:{1}}" class="col-form-label">
        ${{2}}
      </label>
      <div class="">
        ${{1}}
      </div>
      <div class="form-text">
        ${{1}-info}
      </div>
    </div>
    ]]>
  </message>

  <message id="Wt.Auth.field3.active">
    <![CDATA[
    <div class="row mb-3">
      <label for="${id:{1}}" class="col-form-label">
        ${{2}}
      </label>
      <div class="">
        ${{1}}
      </div>
      <div class="form-text">
        ${{1}-info} ${{3} class="btn-link"}
      </div>
    </div>
    ]]>
  </message>

  <message id="Wt.Auth.checkbox.field">
    <![CDATA[
    ${<if:{1}>}
    <div class="mb-3">
    <div class="form-check">
      ${{1}}
      <label for="${id:{1}}" class="form-check-label">
        ${tr:Wt.Auth.{1}}
      </label>
      <div class="form-text">
          ${{1}-info}
      </div>
    </div>
    </div>
    ${</if:{1}>}
    ]]>
  </message>

  <message id="Wt.WNavigationBar.template">
    <div class="container-fluid">
      ${title-link}
      ${collapse-button}
      ${contents}
    </div>
  </message>

  <message id="Wt.WNavigationBar.expand-button">
    <span class="navbar-toggler-icon"></span>
  </message>

  <message id="Wt.WDialog.template">
    <div class="modal-dialog">
      ${layout}
    </div>
  </message>

  <message id="Wt.WDialog.titlebar">
    <h4 class="modal-title">${title}</h4>
  </message>
</messages>
