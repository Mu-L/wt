<?xml version="1.0" encoding="UTF-8"?>
<messages xmlns:if="Wt.WTemplate.conditions">

  <message id="Wt.Auth.form-group">form-group</message>
  <message id="Wt.Auth.form-label">control-label col-sm-2</message>
  <message id="Wt.Auth.form-element">col-sm-8</message>
  <message id="Wt.Auth.form-help">help-block</message>
  <message id="Wt.Auth.form-horizontal">form-horizontal</message>

  <message id="Wt.Auth.form-action.class">form-group</message>

  <message id="Wt.Auth.form-action.in">
    <div class="col-sm-offset-2 col-sm-8">
      ${block:{1}}
    </div>
  </message>

  <message id="Wt.Auth.field.specialized">
    <![CDATA[
    ${block:Wt.Auth.field.base '{1}' '{2}' '{3}' 'form-group' 'control-label col-sm-2' 'col-sm-8' 'help-block'}
    ]]>
  </message>

    <message id="Wt.Auth.checkbox.field">
    <![CDATA[
    ${<if:{1}>}
    <div class="form-group">
      <div class="col-sm-offset-2 col-sm-8">
        <div class="checkbox">
          <label>
            ${{1}} ${tr:Wt.Auth.{1}}
            <span class="help-block">${{1}-info}</span>
          </label>
        </div>
      </div>
    </div>
    ${</if:{1}>}
    ]]>
  </message>

  <message id="Wt.WNavigationBar.template">
  ${<if:theme-style-enabled>}
    <div>
      ${collapse-button}
      <div class="navbar-header">
        ${expand-button}
        ${title-link}
      </div>
      ${contents}
    </div>
  ${</if:theme-style-enabled>}
  ${<if:theme-style-disabled>}
    ${block:Wt.WNavigationBar.template.default}
  ${</if:theme-style-disabled>}
  </message>

</messages>
